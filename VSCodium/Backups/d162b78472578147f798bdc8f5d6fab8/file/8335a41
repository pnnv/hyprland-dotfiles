#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

void snu() {
	int n, m, q; cin >> n >> m >> q;
	vector <int> v(n + 1), p(m);
	for (int i = 0; i < n; i++) {
		int x; cin >> x;
		v[x] = i;
	}
	for(auto &i: p) cin >> i;
	
}

int32_t main() {
	ios::sync_with_stdio(false); cin.tie(0);
	int t; cin >> t;
	while(t--) snu();
}